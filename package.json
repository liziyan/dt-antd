{
  "name": "dt-antd",
  "version": "0.0.1beta1",
  "description": "antd for DTchuxing.com's Background management system",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "rimraf lib && babel src --watch --copy-files --source-maps --extensions .es6,.es,.jsx --out-dir lib",
    "prepublish": "npm run compile",
    "compile": "rimraf lib && babel src --copy-files --source-maps --extensions .es6,.es,.jsx,.js --out-dir lib"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "wyvernnot",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.9.2",
    "immutable": "^3.8.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0"
  }
}
